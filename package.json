{
  "name": "acme",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
