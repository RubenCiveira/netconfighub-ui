    "@types/jasmine": "~5.1.8",
    "@types/uuid": "^10.0.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
